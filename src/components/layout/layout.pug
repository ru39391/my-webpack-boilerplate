include ../wrapper/wrapper

block meta
  - const layoutMod = '';
  - const title = 'Шаблон для быстрого старта';

doctype html
html(lang="ru")
  head
    meta(charset="utf-8")
    meta(name="viewport" content="width=device-width, initial-scale=1.0")

    title #{title}
    meta(name="keywords" content="")
    meta(name="description" content="")

  body.layout(class=layoutMod)
    block header
      include ../header/header

    block wrapper
      +wrapper({ title })

    block footer
      include ../footer/footer
